             <script>
                const highlightCircle = document.querySelector('.highlight-circle');
                const serverCards = document.querySelectorAll('.server-card');
                const circleRect = highlightCircle.getBoundingClientRect();

                function checkHighlight() {
                    serverCards.forEach(server => {
                        const serverRect = server.getBoundingClientRect();
                        const centerPoint = (serverRect.left + serverRect.right) / 2;

                        // If the server card's center is within the circle, apply the highlight class
                        if (centerPoint >= circleRect.left && centerPoint <= circleRect.right) {
                            server.classList.add('highlight');
                        } else {
                            server.classList.remove('highlight');
                        }
                    });
                }

                setInterval(checkHighlight, 100);
            </script>
