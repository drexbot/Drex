    /* Profile Image */
    .profile {
        position: relative;
    }

    .profile img {
        border-radius: 50%;
        width: 50px;
        height: 50px;
        cursor: pointer;
    }

    /* Dropdown Card */
    .card-menu {
        background-color: rgba(36, 40, 50, 1);
        background-image: linear-gradient(139deg, rgba(36, 40, 50, 1) 0%, rgba(37, 28, 40, 1) 100%);
